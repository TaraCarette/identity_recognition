              <script language="4">
                <content>
                  <![CDATA[import sqlite3

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.conn = None
        self.lr = 5

    def get_epsilon(self, user_id):
        epsilon = self.conn.execute("SELECT epsilon FROM preferenceTable WHERE user_id=" + user_id).fetchone()[0]
        return epsilon

    def set_epsilon(self, user_id, epsilon):
        self.conn.execute("UPDATE preferenceTable SET epsilon="+ epsilon +" WHERE user_id=" + user_id)
        self.conn.commit()

    def get_response_value(self, user_id, performed_response):
        response_value = self.conn.execute("SELECT response"+ performed_response +" FROM preferenceTable WHERE user_id=" + user_id).fetchone()[0]
        return int(response_value)

    def update_preference(self, user_id, performed_response, reward):
        # QLearning Update Rule
        new_response_value = self.get_response_value(user_id, performed_response) + self.lr * reward

        # Update in Database
        self.conn.execute("UPDATE preferenceTable SET response" + performed_response + "="+ str(reward) +" WHERE user_id=" + user_id)
        self.conn.commit()

    def onLoad(self):
        # Connect to database
        self.conn = sqlite3.connect('robot_db', check_same_thread=False)

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        # Get parameters
        user_id = self.getParameter("user_id")
        performed_response = self.getParameter("performedResponse")
        reward = self.getParameter("reward")

        # Update preference value
        self.update_preference(user_id, str(performed_response), reward)

        # Print new preferences
        self.print_preferences(user_id)

        # Get epsilon
        epsilon = self.get_epsilon(user_id)
        self.logger.info("Current epsilon value:"+str(epsilon))

        # Update epsilon, always keep a 0.1 chance of exploring
        if epsilon > 0.1:
            epsilon = float(epsilon) - 0.1
            self.set_epsilon(user_id, str(epsilon))

    # will add new face with unique id, default to
    def print_preferences(self, user_id):
        # Get the reward values of all preferences in the table
        response1 = self.conn.execute("SELECT response1 FROM preferenceTable WHERE user_id="+user_id).fetchone()[0]
        response2 = self.conn.execute("SELECT response2 FROM preferenceTable WHERE user_id="+user_id).fetchone()[0]
        response3 = self.conn.execute("SELECT response3 FROM preferenceTable WHERE user_id="+user_id).fetchone()[0]
        response4 = self.conn.execute("SELECT response4 FROM preferenceTable WHERE user_id="+user_id).fetchone()[0]
        response5 = self.conn.execute("SELECT response5 FROM preferenceTable WHERE user_id="+user_id).fetchone()[0]
        responses = [response1, response2, response3, response4, response5]

        self.logger.info("user: " + user_id)
        self.logger.info("response1: " + str(response1))
        self.logger.info("response2: " + str(response2))
        self.logger.info("response3: " + str(response3))
        self.logger.info("response4: " + str(response4))
        self.logger.info("response5: " + str(response5))

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
