                        <script language="4">
                          <content>
                            <![CDATA[import sqlite3
#import sys
#sys.path.append("C:\Users\User\AppData\Local\Programs\Python\Python39\Lib\site-packages")
import numpy as np

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        self.conn = None

    # will add new face with unique id, default to
    def get_response(self, user_id):
        # Get the reward values of all preferences in the table
        response1 = self.conn.execute("SELECT response1 FROM preferenceTable WHERE user_id="+user_id).fetchone()
        response2 = self.conn.execute("SELECT response2 FROM preferenceTable WHERE user_id="+user_id).fetchone()
        response3 = self.conn.execute("SELECT response3 FROM preferenceTable WHERE user_id="+user_id).fetchone()
        response4 = self.conn.execute("SELECT response4 FROM preferenceTable WHERE user_id="+user_id).fetchone()
        response5 = self.conn.execute("SELECT response5 FROM preferenceTable WHERE user_id="+user_id).fetchone()
        responses = [response1, response2, response3, response4, response5]

        # Get response with maximum reward and return that as preference
        response = np.argmax(responses) + 1
        return response

    def get_epsilon(self, user_id):
        epsilon = self.conn.execute("SELECT epsilon FROM preferenceTable WHERE user_id=" + user_id).fetchone()[0]
        return epsilon

    def onLoad(self):
        # Connect to database
        self.conn = sqlite3.connect('robot_db', check_same_thread=False)

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        # Retrieve ID parameter
        user_id = self.getParameter("userID")

        # Retrieve epsilon parameter from database
        epsilon = self.get_epsilon(user_id)

        # Choose exploitation vs exploration
        response = 1
        if np.random.uniform(0, 1) < epsilon:
            # Explore: choose random response
            response = np.random.randint(0, 5)
        else:
            # Exploit: choose response with known highest reward
            response = self.get_response(user_id)

        # Return that preference
        self.response(response)

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
